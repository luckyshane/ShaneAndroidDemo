<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shane.me.shanedemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LinearRecyclerActivity" />
        <activity android:name=".activity.ViewPagerDemoActivity" />
        <activity android:name=".activity.CustomProgressBarActivity" />
        <activity android:name=".activity.PopupWindowDemoActivity" />
        <activity android:name=".activity.ServiceLifeCycleDemoActivity" />
        <activity android:name=".activity.MediaRecordDemoActivity" />

        <service
            android:name=".service.MyService"
            android:enabled="true"
            android:exported="true" />
        <service android:name=".service.NetDiagnosisService" />

        <activity
            android:name=".activity.AnimDemoActivity" />
        <activity android:name=".activity.RippleDemoActivity" />
        <activity android:name=".activity.PanelShowDemoActivity" />
        <activity android:name=".activity.DiagnosisDemoActivity" />
        <activity android:name=".activity.ToastDemoActivity"
            android:theme="@style/Theme.AppCompat.TranslucentActionBar"
            >

        </activity>
        <activity android:name=".activity.VideoDialogDemoActivity" />
        <activity android:name=".activity.ProgressBarExActivity" />
        <activity android:name=".activity.WindowManagerDemoActivity" />
        <activity android:name=".activity.DrawerLayoutDemoActivity" />
        <activity android:name=".activity.FragmentExpandDemoActivity" />
        <activity android:name=".activity.ShadowDemoActivity" />
        <activity android:name=".activity.SignInDemoActivity" />
        <activity android:name=".activity.TestActivity" />
        <activity android:name=".activity.SearchDemoActivity" />
        <activity android:name=".activity.LoadingDemoActivity" />
        <activity android:name=".activity.LoadingDemo2Activity" />
        <activity android:name=".activity.TreeListViewActivity" />
    </application>

</manifest>